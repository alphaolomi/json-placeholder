{
    "name": "alphaolomi/json-placeholder",
    "description": "This is my package json-placeholder",
    "keywords": [
        "alphaolomi",
        "json-placeholder"
    ],
    "homepage": "https://github.com/alphaolomi/json-placeholder",
    "license": "MIT",
    "authors": [
        {
            "name": "Alpha Olomi",
            "email": "alphaolomi@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "saloonphp/saloon": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Json\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Json\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
            "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
