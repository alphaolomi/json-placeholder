{
    "name": "alphaolomi/json-placeholder",
    "description": "Unofficial PHP SDK for JSON Placeholder API, A Free fake API for testing and prototyping.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alpha Olomi"
        }
    ],
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "pestphp/pest": "^1.22"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Json\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ],
        "test": [
            "./vendor/bin/pest"
        ]
    }
}
