{
    "name": "alphaolomi/json-placeholder",
    "description": "PHP SDK for JSON Placeholder API, A Free fake API for testing and prototyping.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alpha Olomi",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "sammyjo20/saloon": "^2.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "pestphp/pest": "^1.22"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Json\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Json\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    }
}
